{
  "name": "angclosuretemplate",
  "version": "0.0.1",
  "description": "Angular Google Closure Template Projects",
  "main": "index.js",
  "scripts": {
    "copy": "node build/copy-app.js",
    "scss": "node build/scss.js",
    "clean": "node build/clean.js",
    "bundle": "node build/bundle.js",
    "compile": "node build/compile.js --debug",
    "compile:prod": "node build/compile.js",
    "watch": "node build/watch.js",
    "server": "http-server -c-1 -o",
    "build": "npm run clean && npm run lint && npm run copy && npm run scss && npm run bundle && npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix app/**.js",
    "start": "npm run build && npm run server",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc_conf.json -d ./docs"
  },
  "keywords": [
    "angular",
    "google",
    "closure"
  ],
  "author": "Kurtis Golding",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.4",
    "angular-route": "^1.6.4",
    "bluebird": "^3.5.0",
    "eslint-config-google": "^0.8.0",
    "fs-extra": "^3.0.0",
    "google-closure-compiler": "^20170423.0.0",
    "jquery": "^3.2.1",
    "js-polyfills": "^0.1.33"
  },
  "devDependencies": {
    "angular": "^1.6.5",
    "angular-mocks": "^1.6.5",
    "angular-ui-router": "^1.0.3",
    "colors": "^1.1.2",
    "concat-files": "^0.1.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.8.0",
    "eslint-plugin-angular": "^2.5.0",
    "jasmine-core": "^2.6.4",
    "jsdoc": "^3.4.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "klaw-sync": "^2.1.0",
    "minimist": "^1.2.0",
    "node-cmd": "^3.0.0",
    "node-sass": "^4.5.3",
    "node-watch": "^0.5.4",
    "tsickle": "^0.23.6",
    "typescript": "^2.3.4"
  }
}
